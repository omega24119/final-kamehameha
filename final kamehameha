 local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

--window
local Window = OrionLib:MakeWindow({Name = "All universe hub ", HidePremium = false, IntroText = "All universe hub", SaveConfig = true, ConfigFolder = "OrionTest"})

--tabs
local creditTab = Window:MakeTab({
  Name = "credits",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

local informationTab = Window:MakeTab({
  Name = "Information",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

local mainTab = Window:MakeTab({
  Name = "main",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

local serectTab = Window:MakeTab({
  Name = "Serect",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})


local autogainTab = Window:MakeTab({
  Name = "auto gain",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})


--section
local Section = mainTab:AddSection({
  Name = "farm"
})


--buttons
mainTab:AddButton({
  Name = "maximum multiple!",
  Callback = function()
      local args = {
        [1] = 99999
      }

game:GetService("ReplicatedStorage").Remotes.DoublePower:FireServer(unpack(args))
          print("button pressed")
    end    
})

